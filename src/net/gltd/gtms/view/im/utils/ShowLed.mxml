<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="14" height="16">
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	<fx:int id="_top">0</fx:int>
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			[Bindable][Embed(source="../assets/show_leds/im_unavailable.png")]
			public var showLed:Class
		]]>
	</fx:Script>
	<!--fx:Binding source="_showLed" destination="imgLed.source" twoWay="false" /-->
	
	<s:Image id="imgLed" source="{showLed}" top="{_top}" verticalAlign="middle" horizontalAlign="center" width="16" height="16"  />
</s:Group>
